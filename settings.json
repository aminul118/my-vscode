{
  "explorer.confirmDelete": false,
  "files.autoSave": "afterDelay",
  "liveServer.settings.donotShowInfoMsg": true,
  "editor.codeActionsOnSave": {},
  "workbench.startupEditor": "none",
  "terminal.integrated.fontSize": 16,
  //! Code Format
  "editor.wordWrap": "on",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "explorer.confirmDragAndDrop": false,
  "git.openRepositoryInParentFolders": "never",
  "cSpell.dictionaryDefinitions": [],
  "workbench.settings.applyToAllProfiles": ["editor.fontFamily"],
  //!  Editor settings
  "workbench.colorCustomizations": {
    "editor.background": "#15171b", //! Most Favorite Background Color
    "editorIndentGuide.activeBackground1": "#FF75D8", //! Left side indicator when select any text
    // "editor.background": "#1b1d22", //! Most Favorite Background Color
    // "editor.background": "#1a1a1f", // 2nd Favorite
    // "editor.background": "#181616", // 3 rd Favorite Black Color
    // "editor.background": "#1d1d1d", // This Is Windows Default
    // "editor.background": "#1b1b1b", // This Is Windows Default
    // "editor.foreground": "#bbbbbb", // Editor Text Default and my fav

    "editor.foreground": "#e0dddd",
    "breadcrumb.background": "#15171b",
    "editorLineNumber.activeForeground": "#FF75D8",
    "editor.lineHighlightBorder": "#94350900",
    "editor.findMatchBorder": "#ff3c01",
    "editor.findMatchBackground": "#831717",
    "editor.findMatchForeground": "#fdfeff",
    "selection.background": "#710933",
    "mergeEditor.conflict.unhandled.minimapOverViewRuler": "#fcba03",

    // ! Cursor
    "editorCursor.foreground": "#FF75D8",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.fontWeight": "normal",

    //!  Tittle Bar or Top of the VS code
    "titleBar.activeBackground": "#0d29465d",
    "titleBar.activeForeground": "#ebe1e1",
    "menu.background": "#212129", //Menu Background
    "menu.selectionBackground": "#04395e",
    "menu.selectionForeground": "#ffffff",
    //!  Tab
    "editorGroupHeader.tabsBackground": "#212129",
    "tab.activeBorder": "#83e6d7",
    "tab.activeForeground": "#83e6d7",
    "tab.inactiveForeground": "#b7bbc0",
    "tab.hoverForeground": "#f3f3f1",
    "tab.inactiveBackground": "#2d2d2d00",
    "tab.activeBackground": "#8f1c1c00",
    "tab.activeBorderTop": "default",
    //!  Side Bar
    "sideBar.background": "#1a1a27",
    "sideBarTitle.background": "#1a1a27",
    "sideBarSectionHeader.background": "#1a1a27",
    // "sideBar.border": "#090a0c",
    "sideBarTitle.foreground": "#ffffff",
    "sideBar.foreground": "#c5c0c0",
    "list.activeSelectionBackground": "#04395e",
    "list.focusAndSelectionOutline": "#04395e",
    "list.activeSelectionForeground": "#31f3b9",
    "list.inactiveSelectionForeground": "#31f3b9",
    "list.inactiveSelectionBackground": "#2d2d3a",
    //!  Activity Bar
    "activityBar.activeBorder": "#FF75D8",
    "activityBar.background": "#0d29465d",
    // "activityBar.activeBackground": "#670e79",
    // "activityBar.border": "#6276b3",
    //!  Terminal Panel
    "panel.background": "#17181d", //Full Panel
    "terminal.background": "#17181d",
    "terminalCursor.foreground": "#FF75D8", // Terminal Cursor
    "panel.border": "#48494b",
    "panelTitle.activeForeground": "#83e6d7",
    "panelTitle.activeBorder": "#83e6d7",
    "panelTitle.inactiveForeground": "#b7bbc0",
    "terminal.foreground": "#cccccc",
    //!  Bottom Bar of the VS code
    "statusBar.background": "#15181c"
  },

  //!  Comment italic Vs code Default
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "comment",
        "settings": {
          "fontStyle": "italic"
          // "foreground": "#6e5b5b"
        }
      }
    ]
  },
  //!  Better Comment Extension setting
  "better-comments.multilineComments": true,
  "better-comments.highlightPlainText": false,
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF75D8",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "#transparent",
      "bold": true,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": true,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": true
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "debug.javascript.terminalOptions": {},
  "liveServer.settings.AdvanceCustomBrowserCmdLine": "",
  "liveServer.settings.ChromeDebuggingAttachment": false,
  "editor.fontVariations": false,
  "editor.fontSize": 18,
  "material-icon-theme.files.associations": {},
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.cursorStyle": "line",
  "json.schemas": [
  
  ],
  "editor.semanticTokenColorCustomizations": {},
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontFamily": "Operator Mono Lig, Fira Code, Consolas, 'Courier New', monospace",
  "editor.fontLigatures": false,
  "cSpell.language": "en,en-US",

  "codesnap.containerPadding": ".1em",
  "editor.cursorBlinking": "expand",
  "workbench.colorTheme": "Dracula Theme Soft",
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "git.autofetch": true,
  "launch": {
    "configurations": [],
    "compounds": []
  },

  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.minimap.enabled": false,
  "editor.stickyScroll.enabled": false,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
